{
  "name": "saas-site",
  "version": "1.0.0",
  "private": true,
  "description": "Saas Starter Site",
  "author": "pixegami",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "aws s3 sync public s3://bonestack.com/ --acl public-read",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E8CNJ3J98G2WV --paths \"/*\"",
    "full-deploy": "npm run build && npm run deploy && npm run invalidate"
  },
